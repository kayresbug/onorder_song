        android:layout_marginRight="17.6dp"
        app:layout_constraintRight_toRightOf="@id/topmenu1"
        app:layout_constraintTop_toTopOf="@id/topmenu1"></ImageView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="246.7dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:ignore="MissingConstraints">
        <ImageView
            android:id="@+id/menu1"
            android:layout_width="226.7dp"
            android:layout_height="280dp"
            android:src="@drawable/home_menu1"
            android:layout_marginLeft="33.3dp"
            tools:ignore="MissingConstraints">
        </ImageView>
        <ImageView
            android:id="@+id/menu2"
            android:layout_width="226.7dp"
            android:layout_height="133.3dp"
            android:src="@drawable/home_menu2"
            android:layout_marginLeft="13.3dp"
            app:layout_constraintLeft_toRightOf="@id/menu1"
            tools:ignore="MissingConstraints">
        </ImageView>
        <ImageView
            android:id="@+id/menu3"
            android:layout_width="226.7dp"
            android:layout_height="133.3dp"
            android:src="@drawable/home_menu3"
            android:layout_marginLeft="13.3dp"
            app:layout_constraintLeft_toRightOf="@id/menu1"
            app:layout_constraintTop_toBottomOf="@id/menu2"
            tools:ignore="MissingConstraints">
        </ImageView>
        <ImageView
            android:id="@+id/menu4"
            android:layout_width="226.7dp"
            android:layout_height="186.7dp"
            android:src="@drawable/home_menu4"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@id/menu1"
            tools:ignore="MissingConstraints">
        </ImageView>
        <ImageView
            android:id="@+id/menu5"
            android:layout_width="226.7dp"
            android:layout_height="186.7dp"
            android:src="@drawable/home_menu5"
            android:layout_marginLeft="13.3dp"
            app:layout_constraintLeft_toRightOf="@id/menu4"
            app:layout_constraintTop_toTopOf="@id/menu4"
            tools:ignore="MissingConstraints">
        </ImageView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/bottom_menu_box"
        android:layout_width="466.7dp"
        android:layout_height="80dp"
        android:background="@drawable/home_top_menu1"
        android:layout_marginBottom="13.3dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"></ImageView>
    <ImageView
        android:id="@+id/bottom_menu1"
        android:layout_width="30.7dp"
        android:layout_height="30.7dp"
        android:background="@drawable/bottom_menu1"
        android:layout_marginTop="23.5dp"
        android:layout_marginLeft="53.3dp"
        app:layout_constraintLeft_toLeftOf="@id/bottom_menu_box"
        app:layout_constraintTop_toTopOf="@id/bottom_menu_box"></ImageView>
    <ImageView
        android:id="@+id/bottom_menu1_tc"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/touchbox"
        app:layout_constraintBottom_toBottomOf="@id/bottom_menu1"
        app:layout_constraintLeft_toLeftOf="@id/bottom_menu1"
        app:layout_constraintRight_toRightOf="@id/bottom_menu1"
        app:layout_constraintTop_toTopOf="@id/bottom_menu1"></ImageView>

    <ImageView
        android:id="@+id/bottom_menu2"
        android:layout_width="30.7dp"
        android:layout_height="30.7dp"
