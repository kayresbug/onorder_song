        app:layout_constraintTop_toTopOf="@+id/topmenubox" />
    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanum_square_round_b"
        android:textStyle="normal"
        android:textSize="28sp"
        android:textColor="#4c4948"
        android:gravity="center_horizontal"
        android:layout_marginLeft="383.3dp"
        android:layout_marginTop="56.7dp"
        android:text="6"
        app:layout_constraintLeft_toLeftOf="@+id/topmenubox"
        app:layout_constraintTop_toTopOf="@+id/topmenubox" />

    <ImageView
        android:layout_width="38.8dp"
        android:layout_height="38.8dp"
