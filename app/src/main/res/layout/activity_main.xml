<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/menu_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/menu_3_fill"
        android:layout_marginTop="29.3dp"
        android:layout_marginRight="35.3dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent">
    </ImageView>

    <ImageView
        android:id="@+id/tb1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/touchbox"
        app:layout_constraintBottom_toBottomOf="@id/menu_3"
        app:layout_constraintLeft_toLeftOf="@id/menu_3"
        app:layout_constraintRight_toRightOf="@id/menu_3"
        app:layout_constraintTop_toTopOf="@id/menu_3"></ImageView>

    <ImageView
        android:id="@+id/topmenubox"
        android:layout_width="466.7dp"
        android:layout_height="120dp"
        android:background="@drawable/home_topmenubox"
        android:layout_marginTop="100dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>
    <ImageView
        android:id="@+id/topmenu1"
        android:layout_width="166.7dp"
        android:layout_height="120dp"
        android:background="@drawable/home_top_menu1"
        app:layout_constraintLeft_toLeftOf="@id/topmenubox"
        app:layout_constraintTop_toTopOf="@id/topmenubox"></ImageView>
    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanum_square_round_r"
        android:lineSpacingExtra="2sp"
        android:textColor="#ffffff"
        android:textSize="16sp"
        android:textStyle="normal"
        android:layout_marginLeft="16.7dp"
        android:layout_marginTop="16.7dp"
        android:text="관리자 님"
        app:layout_constraintLeft_toLeftOf="@+id/topmenu1"
        app:layout_constraintTop_toTopOf="@+id/topmenu1" />
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanum_square_round_r"
        android:lineSpacingExtra="2sp"
        android:textColor="#f9bf3d"
        android:textSize="13.3sp"
        android:textStyle="normal"
        android:layout_marginLeft="16.7dp"
        android:layout_marginBottom="42.7dp"
        android:text="테이블 수"
        app:layout_constraintLeft_toLeftOf="@+id/topmenu1"
        app:layout_constraintBottom_toBottomOf="@+id/topmenu1" />
    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanum_square_round_r"
        android:textStyle="normal"
        android:textSize="21.3sp"
        android:textColor="#f9bf3d"
        android:lineSpacingExtra="-1.3sp"
        android:gravity="center_horizontal"
        android:layout_marginRight="13.3dp"
        android:layout_marginBottom="11.3dp"
        android:text="12/24"
        app:layout_constraintRight_toRightOf="@+id/topmenu1"
        app:layout_constraintBottom_toBottomOf="@+id/topmenu1" />
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanum_square_round_r"
        android:textStyle="normal"
        android:textSize="16sp"
        android:textColor="#8e8e8e"
        android:lineSpacingExtra="-1.3sp"
        android:gravity="center_horizontal"
        android:layout_marginLeft="213.3dp"
        android:layout_marginTop="23.3dp"
        android:text="호출현황"
        app:layout_constraintLeft_toLeftOf="@+id/topmenubox"
        app:layout_constraintTop_toTopOf="@+id/topmenubox" />
    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanum_square_round_b"
        android:textStyle="normal"
        android:textSize="28sp"
        android:textColor="#4c4948"
        android:gravity="center_horizontal"
        android:layout_marginLeft="233.3dp"
        android:layout_marginTop="56.7dp"
        android:text="5"
        app:layout_constraintLeft_toLeftOf="@+id/topmenubox"
        app:layout_constraintTop_toTopOf="@+id/topmenubox" />

    <ImageView
        android:layout_width="1dp"
        android:layout_height="93.3dp"
        android:background="@drawable/splitbar"
        android:layout_marginLeft="317dp"
        android:layout_marginTop="13.7dp"
        app:layout_constraintLeft_toLeftOf="@+id/topmenubox"
        app:layout_constraintTop_toTopOf="@+id/topmenubox" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanum_square_round_r"
        android:textStyle="normal"
        android:textSize="16sp"
        android:textColor="#8e8e8e"
        android:lineSpacingExtra="-1.3sp"
        android:gravity="center_horizontal"
        android:layout_marginLeft="363.3dp"
        android:layout_marginTop="23.3dp"
